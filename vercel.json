{
    "buildCommand": "cd sports-dashboard && npm run build",
    "outputDirectory": "sports-dashboard/.next",
    "installCommand": "cd sports-dashboard && npm install",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": ""
    }
}